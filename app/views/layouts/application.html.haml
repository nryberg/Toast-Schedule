!!! XML iso-8859-1

%html
%head
  %title= @header_text
  = stylesheet_link_tag('http://fonts.googleapis.com/css?family=Droid+Serif')
  = stylesheet_link_tag('http://fonts.googleapis.com/css?family=Droid+Serif:bold')
  = stylesheet_link_tag("blueprint/screen.css")
  = stylesheet_link_tag("blueprint/ie.css")
  = stylesheet_link_tag "blueprint/print.css", :media => "print"
  = stylesheet_link_tag "blueprint/plugins/tabs/screen.css"
  = stylesheet_link_tag("formtastic_changes")
  = stylesheet_link_tag("formtastic")
  = stylesheet_link_tag("special.css")
  = javascript_include_tag 'jquery', 'rails', 'application'
  = javascript_include_tag "jquery.min.js", "application"
  = javascript_include_tag "jquery-ui.min.js", "application"
  = stylesheet_link_tag "jquery-ui.css"
  = csrf_meta_tag
%body
  %p#notice= notice 
  %p#class= alert
  .container
    .span-24.site_title
      .span-13
        %div.log_in_bar TM Schedule
        %div= render "layouts/nav_header"
      .span-10
        %span.log_in_bar= render "layouts/nav_login"
        .span-10
          .span-3.meeting_background 
            = link_to "Meetings", meetings_path
          .span-3.member_background
            =link_to "Members", members_path
    .span-24
      = yield
