%span.span-4
  .span-4#lead_box.member_background Members
%span.span-16
  .span-20#lead_box
    = link_to @club.name, @club, :style => 'text-decoration: none'
  .span-20#header_box Active 
  %table
    %tr
      %th Name
      %th e-mail 
    = render :partial => 'member', :collection => @members_active
  
  .span-20#header_box In-active 
  %table
    %tr
      %th Name
      %th e-mail 
    = render :partial => 'member', :collection => @members_inactive
  = link_to '(New Member)', new_member_path
%br

