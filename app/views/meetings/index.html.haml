.span-4
  .span-4#lead_box.meeting_background Meetings
  #action_box
    .span-4= link_to '(Add)', new_meeting_path()
.span-20.last
  .span-20.last
    .span-4#labeler Upcoming 
    .span-16.last
      %table
        %tr
          %th Date
          %th Theme
        = render @meetings
  .span-20.last
    .span-4#labeler Past
    .span-16.last
      %table.span-16
        %tr
          %th Date
          %th Theme
        = render @past_meetings
